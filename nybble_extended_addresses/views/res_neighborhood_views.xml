<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Lista de Barrios -->
        <record id="view_nybble_neighborhood_tree" model="ir.ui.view">
            <field name="name">res.nybble.neighborhood.tree</field>
            <field name="model">res.neighborhood</field>
            <field name="arch" type="xml">
                <tree string="Barrios">
                    <field name="external_id"/>
                    <field name="name"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <!-- Your Form View Definition -->
        <record id="view_nybble_neighborhood_form" model="ir.ui.view">
            <field name="name">res.nybble.neighborhood.form</field>
            <field name="model">res.neighborhood</field>
            <field name="arch" type="xml">
                <form string="Barrio">
                    <group>
                        <group>
                            <field name="name" string="Nombre del barrio"/>
                        </group>
                        <group>
                            <field name="active"/>
                        </group>
                    </group>
                </form>
            </field>
        </record>

        <!-- Your Action Window Definition -->
        <record id="action_nybble_neighborhood" model="ir.actions.act_window">
            <field name="name">Barrios</field>
            <field name="res_model">res.neighborhood</field>
            <field name="view_mode">list,form</field>
        </record>

        <!-- Action Menu Item Related To Above Action Window -->
        <menuitem
                action="action_nybble_neighborhood"
                id="action_menu_nybble_neighborhood"
                parent="contacts.res_partner_menu_config"
                name="Nybble Barrios"
                sequence="10"/>

    </data>
</odoo>
